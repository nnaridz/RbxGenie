{
  "name": "rbxgenie-daemon",
  "version": "1.0.0",
  "description": "AI Vibe Code daemon for Roblox Studio",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "bundle": "node scripts/bundle.js",
    "bundle:install": "node scripts/bundle.js && copy dist\\RbxGenie.plugin.lua \"%LOCALAPPDATA%\\Roblox\\Plugins\\RbxGenie.lua\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.0",
    "@types/uuid": "^9.0.8",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}